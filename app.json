{
  "expo": {
    "name": "Chacha",
    "scheme": "cha",
    "slug": "Chacha",
    "userInterfaceStyle": "automatic",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo/3/1024.png",
    "splash": {
      "image": "./assets/logo/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/3/res/mipmap-hdpi/3_adaptive_fore.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mantle.Chacha"
    },
    "web": {
      "favicon": "./assets/logo/play_store_512.png"
    },
    "description": "Introducing stories",
    "notification": {
      "icon": "./assets/logo/notif_1.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/notif_1.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}